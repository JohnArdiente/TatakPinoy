@model List<TatakPinoy.Models.Consignee>
@{
    ViewData["Title"] = "Home Page";
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>Presento Bootstrap Template - Index</title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <link href="~/portfolio_boostrap/img/favicon.png" rel="icon">
    <link href="~/portfolio_boostrap/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/portfolio_boostrap/vendor/aos/aos.css" rel="stylesheet">
    <link href="~/portfolio_boostrap/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/portfolio_boostrap/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/portfolio_boostrap/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/portfolio_boostrap/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/portfolio_boostrap/vendor/remixicon/remixicon.css" rel="stylesheet">
    <link href="~/portfolio_boostrap/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/portfolio_boostrap/css/styles.css" rel="stylesheet">
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.1.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.0/js/bootstrap.min.js"></script>
    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
    <style>
                .jumbotron {
        background: #358CCE;
        color: #FFF;
        border-radius: 0px;
        }
        .jumbotron-sm { padding-top: 24px;
        padding-bottom: 24px; }
        .jumbotron small {
        color: #FFF;
        }
        .h1 small {
        font-size: 24px;
        }
    </style>
</head>

<body>

    <!-- ======= Hero Section ======= -->
    <section id="hero" class="d-flex align-items-center">
        <div class="container" data-aos="zoom-out" data-aos-delay="100">
            <div class="row">
                <form asp-controller="Home" asp-action="Track">
                    <div class="col-xl-6 bb">
                        <h1><p style="color:darkblue">Track your cargo</p></h1>
                        <p style="color:maroon;font-style:italic">For easy access,please track your boxes as indicated!</p>
                        <input type="text" name="Track" style="width: 350px; height: 46px" placeholder="ABC-101-245">
                        <button class="btn-get-started scrollto">Search</button>
                    </div>
                </form>
            </div>
        </div>
    </section><!-- End Hero -->

    <main id="main">
        <!-- ======= Portfolio Section ======= -->
        <section id="portfolio" class="portfolio">
            <div class="container" data-aos="fade-up">


            </div>
        </section><!-- End Portfolio Section -->
        <!-- ======= Services Section ======= -->
        <section id="services" class="services section-bg ">
            <div class="container" data-aos="fade-up">

                <div class="section-title">
                    <h2>Services</h2>
                </div>

                <div class="row">
                    <div class="col-md-6">
                        <div class="icon-box" data-aos="fade-up" data-aos-delay="100">
                            <i class="bi bi-briefcase"></i>
                            <h4><a href="#">Balikbayan door to door</a></h4>
                            <p>Please contact us from the following locations:</p>
                            <ol class="list-group list-group-numbered">
                                <li class="list-group-item d-flex justify-content-between align-items-start">
                                    <div class="ms-2 me-auto">
                                        <div class="fw-bold">New York Area:</div>
                                        &nbsp;&nbsp;&nbsp; Efren &nbsp; 856-254-6746<br />
                                        &nbsp;&nbsp;&nbsp; Bong &nbsp; 646-259-0917<br />
                                        &nbsp;&nbsp;&nbsp; Arlyne &nbsp; 201-428-4881
                                    </div>
                                    <span class="badge bg-primary rounded-pill"></span>
                                </li>
                                <li class="list-group-item d-flex justify-content-between align-items-start">
                                    <div class="ms-2 me-auto">
                                        <div class="fw-bold">Jamaica, Queens, N Y</div>
                                        &nbsp;&nbsp;&nbsp; Devie &nbsp; 917-287-242
                                    </div>
                                    <span class="badge bg-primary rounded-pill"></span>
                                </li>
                                <li class="list-group-item d-flex justify-content-between align-items-start">
                                    <div class="ms-2 me-auto">
                                        <div class="fw-bold">Jersey City, Boston, New England</div>
                                        &nbsp;&nbsp;&nbsp; Dennis &nbsp; 201-428-819
                                    </div>
                                    <span class="badge bg-primary rounded-pill"></span>
                                </li>
                            </ol>
                        </div>
                    </div>
                    <div class="col-md-6 mt-4 mt-md-0">
                        <div class="icon-box" data-aos="fade-up" data-aos-delay="200">
                            <i class="bi bi-card-checklist"></i>
                            <h4><a href="#">Lipat Bahay locally moving service</a></h4>
                            <p>Free estimate; mura pa!! We also help pack if needed.</p>
                        </div>
                        <div class="icon-box" data-aos="fade-up" data-aos-delay="300">
                            <i class="bi bi-bar-chart"></i>
                            <h4><a href="#">Pick up and deliver goods or stuff locally.</a></h4>
                        </div>
                        <div class="icon-box" data-aos="fade-up" data-aos-delay="300">
                            <i class="bi bi-bar-chart"></i>
                            <h4><a href="#">Van hire for families, groups or friends.</a></h4>
                        </div>
                    </div>
                    <div class="col-md-6 mt-4 mt-md-0">
                        <div class="icon-box" data-aos="fade-up" data-aos-delay="300">
                            <i class="bi bi-bar-chart"></i>
                            <h4><a href="#">Referals for employment, etc.</a></h4>
                        </div>
                    </div>
                    <div class="col-md-6 mt-4 mt-md-0">
                        <div class="icon-box" data-aos="fade-up" data-aos-delay="400">
                            <i class="bi bi-binoculars"></i>
                            <h4><a href="#">Travel bookings</a></h4>
                        </div>
                    </div>
                </div>

            </div>
        </section><!-- End Services Section -->
        <!-- ======= Contact Section ======= -->
        <section id="contact" class="contact">
            <div class="container" data-aos="fade-up">

                <div class="section-title">
                    <h2>Contact Us</h2>
                </div>

                <div class="row" data-aos="fade-up" data-aos-delay="100">

                    <div class="col-lg-6">

                        <div class="row">
                            <div class="col-md-12">
                                <div class="info-box">
                                    <i class="bx bx-map"></i>
                                    <h3>Our Address</h3>
                                    <p>39-10, 63rd St. , Woodside New York 11377</p>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="info-box mt-4">
                                    <i class="bx bx-envelope"></i>
                                    <h3>Email Us</h3>
                                    <p>tatakpinoyusa@gmail.com</p>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="info-box mt-4">
                                    <i class="bx bx-phone-call"></i>
                                    <h3>Call Us</h3>
                                    <p>856-254-6746<br>646-259-0917<br>201-428-4881</p>
                                </div>
                            </div>
                        </div>

                    </div>

                    <div class="col-lg-6">
                        <div class="well well-sm">
                            <form asp-controller="Contacts" asp-action="Create" method="post" id="message_form">
                                <input name="subject" value="" type="hidden" />
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="name">
                                                Name
                                            </label>
                                            <input type="text" class="form-control" name="name" id="name" placeholder="Enter name" required="required" />
                                        </div>
                                        <div class="form-group">
                                            <label for="email">
                                                Email Address
                                            </label>
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <span class="glyphicon glyphicon-envelope"></span>
                                                </span>
                                                <input type="email" class="form-control" name="email" id="email" placeholder="Enter email" required="required" />
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="email">
                                                Contact Number
                                            </label>
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <span class="glyphicon glyphicon-earphone"></span>
                                                </span>
                                                <input type="email" class="form-control" name="contactno" id="contactno" placeholder="Enter Contact Number" required="required" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="name">
                                                Message
                                            </label>
                                            <textarea name="msg" id="msg" class="form-control" rows="9" cols="25" required="required"
                                                      placeholder="Message"></textarea>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <button type="button" class="btn btn-primary pull-right" id="btnContactUs">
                                            Send Message
                                        </button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>

            </div>
            <br />
            <div class="section-title">
                <h5>Maraming salamat po sa pagtangkilik sa sariling atin. Mabuhay ang Pilipino. Magtulungan para umangat</h5>
                <h4>Pilipinong totoo. Tatak Pinoy Tayo!! May God Bless us All !!!</h4>
            </div>
        </section><!-- End Contact Section -->

    </main><!-- End #main -->


    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/portfolio_boostrap/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="~/portfolio_boostrap/vendor/aos/aos.js"></script>
    <script src="~/portfolio_boostrap/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/portfolio_boostrap/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/portfolio_boostrap/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/portfolio_boostrap/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/portfolio_boostrap/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/portfolio_boostrap/js/main.js"></script>
    <script>
        $("#btnContactUs").click(function () {
            var form = $('#message_form');
            console.log(form.serialize());
            $.ajax({
                url: '@Url.Action("Create","Messages")',
                type: 'POST',
                async: true,
                data: form.serialize(),
                success: function (form) {
                    alert("Successfully Added");
                    $('input[type="text"],textarea').val('');
                    $('input[type="email"]').val('');
                },
                error(result) {
                    console.log(result);
                }
            });
        });

    </script>

</body>

</html>