@model TatakPinoy.Models.ConsigneeStatus

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div class="container">
    <h4>Create New Status</h4>
    <hr />
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label class="control-label">Status </label>
                    <input asp-for="ConsigneeStatusDesc" class="form-control" />
                    <span asp-validation-for="ConsigneeStatusDesc" class="text-danger"></span>
                </div>
                <br />
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-sm btn-outline-success" />
                    <a asp-action="Index" class="btn btn-sm btn-outline-success">Back to List</a>
                </div>
            </form>
        </div>
    </div>

</div>
