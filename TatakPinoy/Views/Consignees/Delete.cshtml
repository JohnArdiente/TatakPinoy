@model TatakPinoy.Models.Consignee

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div class="container">
    <h4>Are you sure you want to delete this?</h4>
    <div>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                Tracking Number :
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.TrackingNo)
            </dd>
            <dt class="col-sm-2">
               Shippers Name :
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ShipersName)
            </dd>
            <dt class="col-sm-2">
                Shippers Number :
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ShipersNo)
            </dd>
            <dt class="col-sm-2">
                Consignee's Name :
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ConsigneesName)
            </dd>
            <dt class="col-sm-2">
                Address :
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ConsigneesAddr)
            </dd>
            <dt class="col-sm-2">
                Consignee's Number :
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ConsigneesNo)
            </dd>
            <dt class="col-sm-2">
                Quantity :
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Qty)
            </dd>
            <dt class="col-sm-2">
                Agent's Name :
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.AgentsName)
            </dd>
            <dt class="col-sm-2">
                Pick Up Date :
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.PickupDate)
            </dd>
            <dt class="col-sm-2">
                Shipment :
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Shipment.ShipmentId)
            </dd>
        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="ConsigneeId" />
            <input type="submit" value="Delete" class="btn btn-outline-danger" />
            <a class="btn  btn-outline-success" asp-action="Index">Back to List</a>
        </form>
    </div>
</div>
